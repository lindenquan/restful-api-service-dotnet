{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDB": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "prescription_orders"
  },
  "Cache": {
    "L1": {
      "Enabled": true,
      "TtlSeconds": 30,
      "MaxItems": 1000
    },
    "L2": {
      "Enabled": true,
      "ConnectionString": "localhost:6379",
      "InstanceName": "PrescriptionApi:E2E:",
      "TtlSeconds": 60
    }
  },
  "RootAdmin": {
    "Email": "admin@e2e-test.local",
    "InitialApiKey": "e2e-test-admin-api-key-12345",
    "EnableAutoCreate": true
  }
}

