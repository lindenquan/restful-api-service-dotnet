{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Application": "Debug"
    }
  },

  "MongoDB": {
    "ConnectionString": "mongodb://staging-mongo:27017",
    "DatabaseName": "prescription_orders"
  },

  "Cache": {
    "L1": {
      "Enabled": true,
      "Consistency": "Strong",
      "TtlSeconds": 15,
      "MaxItems": 20000
    },
    "L2": {
      "Enabled": true,
      "Consistency": "Strong",
      "ConnectionString": "staging-redis:6379",
      "InstanceName": "PrescriptionApi:Stage:",
      "TtlSeconds": 180
    }
  },

  "RootAdmin": {
    "Email": "admin@staging.local",
    "InitialApiKey": "",
    "EnableAutoCreate": true
  },

  "Cors": {
    "AllowedOrigins": [],
    "AllowCredentials": false
  },

}

